{
  "name": "expo-assistant",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "author": {
    "name": "Piguave Alvarado Jean Carlos",
    "email": "piguavejc@gmail.com",
    "url": "https://piguavesof.com"
  },
  "repository": {
    "directory": "expo-assistant",
    "type": ".git",
    "url": "https://github.com/jean-carlos-19/expo-assistant"
  },
  "config": {
    "port":3000
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettier.json",
    "build-dev":"eas build -p android --profile development",
    "build-prod":"eas build -p android --profile production"
  },
  "dependencies": {
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "axios": "^1.5.0",
    "chatgpt": "^5.2.5",
    "expo": "~49.0.8",
    "expo-av": "^13.4.1",
    "expo-dev-client": "2.4.8",
    "expo-permissions": "^14.2.1",
    "expo-speech": "~11.3.0",
    "expo-status-bar": "~1.6.0",
    "link": "^1.5.1",
    "nativewind": "^2.0.11",
    "openai": "^4.3.1",
    "react": "18.2.0",
    "react-native": "^0.72.4",
    "react-native-dotenv": "^3.4.9",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-tts": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/axios": "^0.14.0",
    "@types/react": "~18.2.14",
    "@types/react-native-dotenv": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "prettier": "^3.0.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
